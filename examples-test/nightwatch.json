{
  "src_folders": ["./examples-test/leaflet", "./examples-test/openlayers"],
  "output_folder": "./examples-test/reports",
  "exclude": ["./examples-test/leaflet/tiandituLayer_llIT.js", "./examples-test/leaflet/tiandituLayer_mercatorIT.js"],
  "filter": "",
  "custom_commands_path": "",
  "custom_assertions_path": "",
  "page_objects_path": "",
  "globals_path": "",

  "selenium": {
    "start_process": true,
    "server_path": "./node_modules/selenium-server/lib/runner/selenium-server-standalone-3.141.59.jar",
    "log_path": false,
    "host": "127.0.0.1",
    "port": 4445,
    "cli_args": {
      "webdriver.chrome.driver": "./node_modules/chromedriver/lib/chromedriver/chromedriver.exe"
    }
  },

  "test_settings": {
    "default": {
      "launch_url": "http://localhost",
      "selenium_port": 4445,
      "selenium_host": "localhost",
      "silent": true,
      "screenshots": {
        "enabled": false,
        "on_failure": false,
        "on_error": false,
        "path": ""
      },
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "chromeOptions": {
          "w3c": false,
          "args": ["--disable-gpu --no-sandbox --headless --window-size=1920,1080 --verbose"]
        }
      }
    },

    "chrome": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "chromeOptions": {
          "w3c": false
        }
      }
    }
  }
}
